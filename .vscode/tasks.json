{
    // See https://go.microsoft.com/fwlink/?LinkId=733558 
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
      {
        "type": "shell",
        "label": "cargo run example print_tree",
        "command": "cargo",
        "args": [
            "run",
            "--example",
            "print_tree"
        ],
        "problemMatcher": [
            "$rustc"
        ],
        "group": "build"
      },
      {
        "type": "shell",
        "label": "cargo build example print_tree",
        "command": "cargo",
        "args": [
            "build",
            "--example",
            "print_tree"
        ],
        "problemMatcher": [
            "$rustc"
        ],
        "group": "build"
      },
    ]
  }